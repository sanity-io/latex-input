{
  "name": "sanity-plugin-latex-input-next-compatible",
  "author": "Filipe <hello@filipesm.eu>",
  "version": "0.4.2-v3-studio.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipesmedeiros/latex-input.git"
  },
  "source": "./src/index.tsx",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier src -w",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/latex-input"
  },
  "keywords": [
    "sanity",
    "plugin",
    "latex"
  ],
  "homepage": "https://github.com/sanity-io/latex-input#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.3.5"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@types/katex": "^0.11.0",
    "@types/styled-components": "^5.1.25",
    "katex": "^0.11.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/plugin-kit": "^1.1.0-ecosystem-preset.5",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@types/jest": "^27.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.0",
    "prettier": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.22",
    "standard-version": "^9.3.2",
    "styled-components": "^5.3.5",
    "typescript": "4.7.4"
  }
}
